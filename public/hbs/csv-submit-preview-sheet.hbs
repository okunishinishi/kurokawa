<table class="wide-table">
    <thead>
    <tr>
        {{#each headers}}
            <th>{{this}}</th>
        {{/each}}
    </tr>
    </thead>
    <tbody>
    {{#each rows}}
        <tr>
            {{#each this}}
                {{#if this}}
                <td class="{{style_class}}">
                    <div class="preview-sheet-content">
                        {{this}}
                    </div>
                </td>
                {{else}}
                    <td></td>
                {{/if}}
            {{/each}}
        </tr>
    {{/each}}
    </tbody>
</table>
